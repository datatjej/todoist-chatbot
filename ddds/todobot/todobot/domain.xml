<?xml version="1.0" encoding="utf-8"?>
<domain name="TodobotDomain" is_super_domain="true">
  <goal type="perform" action="top">
    <plan>
      <forget_all/>
      <findout type="goal"/>
    </plan>
  </goal>
  
  <!--"get my projects" -->
  <goal type="resolve" question_type="wh_question" predicate="project_to_show">
    <plan>
	  <invoke_service_query type="wh_question" predicate="project_to_show"/>
    </plan>
  </goal>
  
  <!--"create a projects" -->
  <goal type="perform" action="create_project">
    <plan>
	  <findout type="wh_question" predicate="project_to_add"/>
	  <invoke_service_action name="CreateProject" postconfirm="true"/>
    </plan>
  </goal>
  
  
</domain>
